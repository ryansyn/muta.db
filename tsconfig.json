{
	"compilerOptions": {
		"target": "ESNEXT",
		"module": "ESNext",
		"declaration": true,
		"noEmit": true,
		"strict": true,
		"moduleResolution": "node",
		"typeRoots": [
			"./node_modules/@types",
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"node_modules",
		".build"
	]
}
